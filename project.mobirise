          "_customHTML": "<section class=\"tabs1\" group=\"Tabs\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-tabs\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"9\" step=\"1\" value=\"9\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"9\" step=\"1\" value=\"6\">\n\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"color\" title=\"Tabs Color\" name=\"tabsColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Active Tab Color\" name=\"activeTabColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Tabs Bg\" name=\"tabsBg\" value=\"#ffffff\">\n        <select title=\"Tabs Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected>4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"range\" title=\"Tab 1 Items\" inline name=\"items1\" min=\"1\" max=\"50\" step=\"1\" value=\"6\">\n        <input type=\"range\" title=\"Tab 2 Items\" inline name=\"items2\" min=\"1\" max=\"50\" step=\"1\" value=\"4\" condition=\"bootstrapTabs>1\">\n        <input type=\"range\" title=\"Tab 3 Items\" inline name=\"items3\" min=\"1\" max=\"50\" step=\"1\" value=\"5\" condition=\"bootstrapTabs>2\">\n        <input type=\"range\" title=\"Tab 4 Items\" inline name=\"items4\" min=\"1\" max=\"50\" step=\"1\" value=\"3\" condition=\"bootstrapTabs>3\">\n        <input type=\"range\" title=\"Tab 5 Items\" inline name=\"items5\" min=\"1\" max=\"50\" step=\"1\" value=\"4\" condition=\"bootstrapTabs>4\">\n        <input type=\"range\" title=\"Tab 6 Items\" inline name=\"items6\" min=\"1\" max=\"50\" step=\"1\" value=\"4\" condition=\"bootstrapTabs>5\">\n        <input type=\"range\" title=\"Tab 7 Items\" inline name=\"items7\" min=\"1\" max=\"50\" step=\"1\" value=\"4\" condition=\"bootstrapTabs>6\">\n        <input type=\"range\" title=\"Tab 8 Items\" inline name=\"items8\" min=\"1\" max=\"50\" step=\"1\" value=\"4\" condition=\"bootstrapTabs>7\">\n\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/arabica-aromatic-beverage-678654-2000x1594.jpg\" selected>\n            <input type=\"color\" title=\"Background Color\" value=\"#efefef\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=pjiVuLVqWQg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Gradient\" name=\"gradientBg\" condition=\"bg.type=='color'\" checked>\n        <input type=\"color\" title=\"Color 2\" name=\"color2\" value=\"#ffffff\" condition=\"gradientBg && bg.type == 'color'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#137ce1\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.3\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <h3 class=\"mbr-section-title alig